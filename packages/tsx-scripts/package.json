{
  "name": "tsx-scripts",
  "version": "0.0.4",
  "description": "tsx-script build cli",
  "type": "module",
  "bin": {
    "tsx-scripts": "./dist/index.cjs"
  },
  "main": "./dist/index.cjs",
  "scripts": {
    "start": "cross-env NODE_ENV=development CUSTOM_ENV=development ts-node --esm ./bin/index.ts dev",
    "build": "rimraf dist && ts-node --esm ./webpack.config.ts",
    "test": "yarn build && node ./dist/index.cjs build"
  },
  "license": "MIT",
  "author": "harhao",
  "dependencies": {
    "@sentry/webpack-plugin": "^2.7.1",
    "@types/node": "^20.3.1",
    "@types/webpackbar": "^4.0.3",
    "autoprefixer": "^10.4.14",
    "chalk": "^4.1.0",
    "cleye": "^1.3.2",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.9",
    "glob": "^10.3.3",
    "html-webpack-plugin": "^5.5.3",
    "http-server": "^14.1.1",
    "ip": "^1.1.8",
    "json-server": "^0.17.3",
    "less": "^4.1.3",
    "less-loader": "^11.1.3",
    "mini-css-extract-plugin": "^2.7.6",
    "ora": "^5.1.0",
    "postcss-loader": "^7.3.3",
    "postcss-px-to-viewport": "^1.1.1",
    "purgecss-webpack-plugin": "^5.0.0",
    "rimraf": "^5.0.1",
    "semver": "^7.5.4",
    "shelljs": "^0.8.5",
    "style-loader": "^3.3.3",
    "terser": "^5.18.0",
    "terser-webpack-plugin": "^5.3.9",
    "thread-loader": "^4.0.2",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3",
    "webpack": "^5.88.1",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^5.9.0",
    "webpack-node-externals": "^3.0.0",
    "webpackbar": "^5.0.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "ie 9"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "ie 9"
    ],
    "sideEffects": [
      "*.css"
    ]
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
