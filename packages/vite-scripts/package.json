{
  "name": "vite-scripts",
  "version": "1.0.0",
  "description": "vite-script build cli",
  "type": "module",
  "bin": {
    "vite-scripts": "./dist/index.cjs"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development CUSTOM_ENV=development ts-node --esm ./bin/index.ts dev",
    "build": "rimraf dist && ts-node --esm ./webpack.config.ts",
    "test": "yarn build && node ./dist/index.cjs dev"
  },
  "license": "MIT",
  "author": "harhao",
  "dependencies": {
    "cleye": "^1.3.2",
    "semver": "^7.5.4"
  },
  "devDependencies": {
    "@types/node": "^20.3.1",
    "@vitejs/plugin-react": "^4.0.4",
    "autoprefixer": "^10.4.14",
    "chalk": "^5.3.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.9",
    "glob": "^10.3.3",
    "http-server": "^14.1.1",
    "internal-ip": "^8.0.0",
    "json-server": "^0.17.3",
    "less": "^4.1.3",
    "ora": "^6.3.1",
    "postcss-px-to-viewport": "^1.1.1",
    "rimraf": "^5.0.1",
    "shelljs": "^0.8.5",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3",
    "vite": "^4.4.9",
    "webpack": "^5.88.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "ie 9"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "ie 9"
    ],
    "sideEffects": [
      "*.css"
    ]
  }
}
